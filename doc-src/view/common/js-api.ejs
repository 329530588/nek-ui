<h3>API</h3>

<h4>Class</h4>
<h5><%= class_.name %></h5>
<p>继承自<strong><%= class_.extend %></strong>。</p>
<p><%= class_.description %></p>
<div id="j-class"></div>

<h4>Options</h4>
<h5>new <%= class_.name %>()</h5>
<table>
    <thead>
        <tr><th>参数</th><th>类型</th><th>默认值</th><th>描述</th></tr>
    </thead>
    <tbody>
        <% if(class_.params.length) { %>
            <% class_.params.forEach(function(param) { %>
            <tr><td class="f-wsn"><code><%= param.name %></code></td><td class="f-wsn"><%= param.type %></td><td class="f-wsn"><% if(param.default_) { %><code><%= param.default_ %></code><% } %></td><td><%= param.description %></td></tr>
            <% }); %>
        <% } else { %>
            <tr><td class="f-tac" colspan="4">无参数</td></tr>
        <% } %>
    </tbody>
</table>
<div id="j-options"></div>

<% if(methods.length) { %>
    <h4>Methods</h4>
    <% methods.forEach(function(method) { %>
    <h5><%= class_.lowerName %>.<%= method.name %>(<%= method.body %>)</h5>
    <p><%= method.description %></p>
    <table>
        <thead>
            <tr><th>参数</th><th>类型</th><th>默认值</th><th>描述</th></tr>
        </thead>
        <tbody>
            <% if(method.params.length) { %>
                <% method.params.forEach(function(param) { %>
                <tr><td class="f-wsn"><code><%= param.name %></code></td><td class="f-wsn"><%= param.type %></td><td class="f-wsn"><% if(param.default_) { %><code><%= param.default_ %></code><% } %></td><td><%= param.description %></td></tr>
                <% }); %>
            <% } else { %>
                <tr><td class="f-tac" colspan="4">无参数</td></tr>
            <% } %>
        </tbody>
    </table>
    <% }); %>
<% } %>
<div id="j-methods"></div>

<% if(staticMethods.length) { %>
    <h4>Static Methods</h4>
    <% staticMethods.forEach(function(method) { %>
    <h5><%= class_.name %>.<%= method.name %>(<%= method.body %>)</h5>
    <p><%= method.description %></p>
    <table>
        <thead>
            <tr><th>参数</th><th>类型</th><th>默认值</th><th>描述</th></tr>
        </thead>
        <tbody>
            <% if(method.params.length) { %>
                <% method.params.forEach(function(param) { %>
                <tr><td class="f-wsn"><code><%= param.name %></code></td><td class="f-wsn"><%= param.type %></td><td class="f-wsn"><% if(param.default_) { %><code><%= param.default_ %></code><% } %></td><td><%= param.description %></td></tr>
                <% }); %>
            <% } else { %>
                <tr><td class="f-tac" colspan="4">无参数</td></tr>
            <% } %>
        </tbody>
    </table>
    <% }); %>
<% } %>
<div id="j-staticMethods"></div>

<% if(inheritedMethods.length) { %>
    <h4>Inherited Methods</h4>
    <% inheritedMethods.forEach(function(method) { %>
    <% if(!method.overridden) { %>
        <h5><%= class_.lowerName %>.<%= method.name %>(<%= method.body %>)</h5>
        <p><%= method.description %></p>
        <table>
            <thead>
                <tr><th>参数</th><th>类型</th><th>默认值</th><th>描述</th></tr>
            </thead>
            <tbody>
                <% if(method.params.length) { %>
                    <% method.params.forEach(function(param) { %>
                    <tr><td class="f-wsn"><code><%= param.name %></code></td><td class="f-wsn"><%= param.type %></td><td class="f-wsn"><% if(param.default_) { %><code><%= param.default_ %></code><% } %></td><td><%= param.description %></td></tr>
                    <% }); %>
                <% } else { %>
                    <tr><td class="f-tac" colspan="4">无参数</td></tr>
                <% } %>
            </tbody>
        </table>
    <% } else { %>
        <h5><span class="u-text u-text-muted">@override</span> <del><%= class_.lowerName %>.<%= method.name %>(<%= method.body %>)</del></h5>
    <% } %>
    <% }); %>
<% } %>
<div id="j-inheritedMethods"></div>

<% if(events.length) { %>
    <h4>Events</h4>
    <% events.forEach(function(event) { %>
    <h5><%= class_.lowerName %>.$on('<%= event.name %>')</h5>
    <p><%= event.description %></p>
    <table>
        <thead>
            <tr><th>属性</th><th>类型</th><th>描述</th></tr>
        </thead>
        <tbody>
            <% if(event.properties.length) { %>
                <% event.properties.forEach(function(property) { %>
                <tr><td class="f-wsn"><code><%= property.name %></code></td><td class="f-wsn"><%= property.type %></td><td class="f-wsn"><%= property.description %></td></tr>
                <% }); %>
            <% } else { %>
                <tr><td class="f-tac" colspan="4">无参数</td></tr>
            <% } %>
        </tbody>
    </table>
    <% }); %>
<% } %>
<div id="j-events"></div>

<% if(inheritedEvents.length) { %>
    <h4>Inherited Events</h4>
    <% inheritedEvents.forEach(function(event) { %>
    <% if(!event.overridden) { %>
        <h5><%= class_.lowerName %>.$on('<%= event.name %>')</h5>
        <p><%= event.description %></p>
        <table>
            <thead>
                <tr><th>属性</th><th>类型</th><th>描述</th></tr>
            </thead>
            <tbody>
                <% if(event.properties.length) { %>
                    <% event.properties.forEach(function(property) { %>
                    <tr><td class="f-wsn"><code><%= property.name %></code></td><td class="f-wsn"><%= property.type %></td><td class="f-wsn"><%= property.description %></td></tr>
                    <% }); %>
                <% } else { %>
                    <tr><td class="f-tac" colspan="4">无参数</td></tr>
                <% } %>
            </tbody>
        </table>
    <% } else { %>
        <h5><span class="u-text u-text-muted">@override</span> <del><%= class_.lowerName %>.$on('<%= event.name %>')</del></h5>
    <% } %>
    <% }); %>
<% } %>
<div id="j-inheritedEvents"></div>