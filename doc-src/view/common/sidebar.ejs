<div class="g-sd">
    <ul class="m-sidebar">
        <% var group; %>
        <% sidenavs.forEach(function(item) { %>
        <% if(item.hidden) return; %>
        <% if(item.group != group) {
            group = item.group; %>
        <li class="sidebar_group"><span><%= item.group %></span></li>
        <% } %>
        <li<% if(sidenav === item.lowerName || sidenav === item.source) { %> class="z-crt"<% } %>><a href="<%= item.path %>" <%= item.blank ? 'target="_blank"' : ''%>><%= item.cnName %><small> <%= item.name %></small>
        </a></li>
        <% }); %>
    </ul>
</div>